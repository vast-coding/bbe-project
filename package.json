{
  "name": "bbe-project",
  "version": "1.0.0",
  "author": "Vasko Trajkovski",
  "description": "",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "types": "dist/typings/index.d.ts",
  "files": [
    "/dist",
    "*.md"
  ],
  "scripts": {
    "build:docs": "build-storybook",
    "build:js": "rollup --config rollup.config.js",
    "build:ts": "tsc -p tsconfig.build.json",
    "build": "npm run build:js && npm run build:ts",
    "lint:fix": "prettier  --ignore-unknown --no-error-on-unmatched-pattern --write \"src/**/*\"",
    "lint:eslint": "eslint src/** --no-error-on-unmatched-pattern",
    "lint:prettier": "prettier --check --no-error-on-unmatched-pattern \"src/**/*\"",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "new": "node scripts/node-cli/generate-react-component.js $1 $2",
    "start:cache": "start-storybook --no-manager-cache -p 6006",
    "start": "start-storybook -p 6006",
    "test:unit:watch": "jest --env=jsdom --watch",
    "test:coverage": "jest --env=jsdom --coverage",
    "test:jest": "jest -w 1 --passWithNoTests --env=jsdom",
    "test:ts": "tsc",
    "test": "npm run test:ts && npm run test:jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy": "./deploy.sh"
  },
  "keywords": [],
  "license": "UNLICENSED",
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "date-fns": "^2.21.0",
    "lodash.get": "^4.4.2",
    "nanoid": ">= 2.0.0",
    "polished": "^4.1.3",
    "prop-types": "^15.6.0",
    "react-icons": "^4.3.1",
    "react-slider": "^1.3.1",
    "react-swipeable": "^6.1.2",
    "rebass": "^3.1.1",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@storybook/addon-actions": "^6.4.4",
    "@storybook/addon-essentials": "^6.4.4",
    "@storybook/addon-links": "^6.4.4",
    "@storybook/react": "^6.4.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-slider": "^1.3.1",
    "@types/styled-components": "^5.1.10",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "bootstrap": "^5.0.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-storybook": "^0.5.1",
    "husky": "^7.0.1",
    "jest": "^27.0.4",
    "jest-junit": "^12.2.0",
    "jest-styled-components": "^7.0.4",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-router-dom": "^5.2.0",
    "rollup": "^2.51.2",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-visualizer": "^5.5.0",
    "ts-jest": "^27.0.3",
    "typescript": "4.2.2",
    "yalc": "^1.0.0-pre.53"
  },
  "peerDependencies": {
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "styled-components": "^5.3.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "eslint --cache --fix",
      "prettier --write",
      "npm run test:jest",
      "npm run lint"
    ],
    "*.{json,md}": "prettier --write"
  }
}
